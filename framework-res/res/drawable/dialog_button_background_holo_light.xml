<?xml version="1.0" encoding="utf-8"?>
<selector android:exitFadeDuration="@integer/dialog_config_mediumAnimTime"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true" android:state_enabled="false" android:state_pressed="true" android:drawable="@drawable/dialog_button_disabled_focused_holo_light" />
    <item android:state_focused="true" android:state_enabled="false" android:drawable="@drawable/dialog_button_disabled_focused_holo_light" />
    <item android:state_focused="false" android:state_enabled="false" android:drawable="@drawable/dialog_button_disabled_holo_light" />
    <item android:state_focused="true" android:state_pressed="true" android:drawable="@drawable/dialog_button_pressed_holo_light" />
    <item android:state_focused="false" android:state_pressed="true" android:drawable="@drawable/dialog_button_longpressed_holo_light" />
    <item android:state_focused="true" android:drawable="@drawable/dialog_button_focused_holo_light" />
    <item android:state_focused="false" android:state_enabled="true" android:drawable="@drawable/dialog_button_default_normal_holo_light" />
